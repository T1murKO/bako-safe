{
    "name": "@infinitybase/sdk",
    "version": "0.0.79",
    "author": "InfinityBase - Guilherme Roque",
    "license": "Apache-2.0",
    "main": "src/index.ts",
    "engines": {
        "node": ">=18.14.1",
        "pnpm": ">=8"
    },
    "publishConfig": {
        "access": "public",
        "main": "dist/index.js",
        "module": "dist/index.mjs",
        "types": "dist/index.d.ts",
        "typings": "dist/index.d.ts",
        "exports": {
            ".": {
                "require": "./dist/index.js",
                "types": "./dist/index.d.ts",
                "default": "./dist/index.mjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build:sdk": "yarn build:esm && yarn build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
        "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --runInBand --detectOpenHandles --forceExit --verbose"
    },
    "devDependencies": {
        "@types/elliptic": "^6.4.14",
        "@types/jest": "^29.5.4",
        "@types/node": "^16.18.11",
        "@types/react": "17.0.37",
        "@types/react-dom": "17.0.11",
        "jest": "^29.6.4",
        "prettier": "3.0.3",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "turbo": "^1.10.0",
        "typescript": "^4.9.4"
    },
    "peerDependencies": {
        "fuels": "0.63.0"
    },
    "dependencies": {
        "@infinitybase/sway": "*",
        "@types/mocha": "^10.0.1",
        "@types/uuid": "^9.0.6",
        "axios": "^1.5.1",
        "elliptic": "^6.5.4",
        "fuels": "0.63.0",
        "pnpm": "^8.10.0",
        "tsup": "^7.2.0",
        "uuid": "^9.0.1"
    }
}
